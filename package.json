{
  "name": "emphasis-roll",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "build": "ts-node ./builder.ts build",
    "package": "ts-node ./builder.ts buildZip",
    "compileReadme": "ts-node ./builder.ts compileReadme",
    "dev": "ts-node ./builder.ts watch",
    "dev11": "ts-node ./builder.ts watch --fi v11",
    "dev12": "ts-node ./builder.ts watch --fi v12",
    "patch": "ts-node ./builder.ts publish -u patch",
    "minor": "ts-node ./builder.ts publish -u minor",
    "major": "ts-node ./builder.ts publish -u major",
    "reupload": "ts-node ./builder.ts reupload",
    "updateSrcPacks": "ts-node ./builder.ts updateSrcPacks",
    "updateZipManifestForGithub": "ts-node ./builder.ts updateZipManifestForGithub",
    "updateExternalManifestForGithub": "ts-node ./builder.ts updateExternalManifestForGithub"
